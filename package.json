    "react-bootstrap": "^0.31.0",
    "react-codemirror": "^0.3.0",
    "react-dom": "^15.5.4",
    "react-immutable-proptypes": "^2.1.0",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "redux-immutable": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "serve-favicon": "^2.4.2"
  }
}
