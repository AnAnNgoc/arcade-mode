{
  "name": "arcade-mode",
  "version": "1.0.0",
  "description": "An interview preparation app featuring algorithm and data structure questions.",
  "main": "server/server.js",
  "scripts": {
    "build": "NODE_ENV=production gulp build",
    "build-dev": "NODE_ENV=development gulp build-dev",
    "postinstall": "npm run build",
    "start": "node --use-strict server/server.js",
    "test": "NODE_ENV=test mocha --require test/helpers/browser.js --compilers .:babel-core/register --recursive test",
    "test:coverage": "NODE_ENV=test bin/cover_test_client.sh && bin/cover_test_server.sh",
    "test:client": "NODE_ENV=test mocha --require test/helpers/browser.js --compilers .:babel-core/register --opts test/client/**/*.{js,jsx} --recursive test/client",
    "test:server": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive test/server",
    "watch": "NODE_ENV=production gulp watch",
    "watch-dev": "NODE_ENV=development gulp watch-dev"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/freeCodeCamp/arcade-mode.git"
  },
  "author": "freeCodeCamp",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/freeCodeCamp/arcade-mode/issues"
  },
  "homepage": "https://github.com/freeCodeCamp/arcade-mode#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-istanbul": "^0.12.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.3.0",
    "browserify-incremental": "^3.1.1",
    "bundle-collapser": "^1.2.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "chai-immutable": "^1.6.0",
    "dotenv": "^4.0.0",
    "envify": "^4.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "event-stream": "^3.3.4",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.1",
    "gulp-cache": "^0.4.6",
    "gulp-cssmin": "^0.1.7",
    "gulp-imagemin": "^3.2.0",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^2.1.2",
    "gulp-util": "^3.0.8",
    "istanbul": "^0.4.5",
    "jsdom": "^10.1.0",
    "mocha": "^3.3.0",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "sinon": "^2.2.0",
    "tiny-worker": "^2.1.1",
    "uglifyify": "^3.0.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "codemirror": "^5.25.2",
    "compression": "^1.6.2",
    "connect-mongo": "^1.3.2",
    "express": "^4.15.2",
    "express-session": "^1.15.2",
    "helmet": "^3.6.0",
    "immutable": "^3.8.1",
    "mongoose": "^4.9.7",
    "morgan": "^1.8.1",
    "prop-types": "^15.5.8",
